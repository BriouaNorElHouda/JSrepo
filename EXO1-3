import React, { useState } from "react";


function App() {
  const [count, setCount] = useState(0);
  const incrementCount = () => {
    setCount(count + 1);
  };
  return (
    <div className="app">
      <button onClick={incrementCount}>Click</button>
      <h1>Compteur {count}</h1>
      
    </div>
  );
}

export default App;
