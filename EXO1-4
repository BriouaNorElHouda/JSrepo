function App() {
    const [count, setCount] = useState(0);
    const incrementCount = () => {
      setCount(count + 1);
    };
    const decrementCount = () => {
      setCount(count - 1);
    };
    return (
      <div >
        <button onClick={incrementCount}>Inc</button>
        <button onClick={decrementCount}>Dec</button>
        <h1>Compteur {count}</h1>
        
      </div>
    );
  }
  export default App;
